{
    "name": "crabstudio/bao-kim",
    "description": "BaoKim payment gateway plugin for CakePHP 3.x",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/crabstudio/bao-kim",
    "license": "MIT",
    "keywords": [
        "cakephp3",
        "cake3",
        "search",
        "crabstudio",
        "anhtuank7c",
        "BaoKim",
        "Payment gateway"
    ],
    "suggest": {
        "crabstudio/sluggable": "Auto create utf-8 slug. Best solution for Vietnamese slug CakePHP 3.x",
        "crabstudio/recaptcha": "Simple google recaptcha for CakePHP 3.x",
        "crabstudio/search": "Easy search solution for CakePHP 3.x. Datetime data type already supported",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "author": [
        {
            "name": "anhtuank7c",
            "email": "anhtuank7c@hotmail.com",
            "role": "Author",
            "homepage": "http://crabstudio.info/"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Crabstudio\\BaoKim\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crabstudio\\BaoKim\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support": {
        "source": "https://github.com/crabstudio/bao-kim",
        "issues": "https://github.com/crabstudio/bao-kim/issues"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
